-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: pre-build main-build

# Main-build Target
main-build: libfand.so

# Tool invocations
libfand.so: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: GCC C++ Linker'
	g++ -shared -o"libfand.so" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(OBJS)$(C++_DEPS)$(C_DEPS)$(CC_DEPS)$(LIBRARIES)$(CPP_DEPS)$(CXX_DEPS)$(C_UPPER_DEPS) libfand.so
	-@echo ' '

pre-build:
	-cd ..; lex -olex.yy.cpp fand.l; yacc -v -d -o y.tab.cpp fand.y
	-@echo ' '

post-build:
	-rm -f y.tab.d lex.yy.d; cd ..; rm -f lex.yy.cpp y.tab.cpp y.tab.cpp.h
	-@echo ' '

.PHONY: all clean dependents
.SECONDARY: main-build pre-build post-build

-include ../makefile.targets
